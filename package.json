{
  "name": "media-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "ng serve --host=0.0.0.0 --port=4200 --disable-host-check",
    "dev": "tsc -w & npm run scss & nodemon",
    "fulldev": "npm run dev & npm run copy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "build": "npm run clean & tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && npm run scssbuild",
    "clean": "rm -rf dist/*",
    "copy": "node src/copy.js",
    "copyonce": "node src/copyOnce.js",
    "scss": "sass --watch src/styles/web:src/web/styles src/styles/common:src/web/styles/common",
    "scssbuild": "sass src/styles/web:src/web/styles src/styles/common:src/web/styles/common"
  },
  "keywords": [],
  "author": "Klaus Wiedenmann",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0",
    "node-sass": "^5.0.0",
    "rxjs": "~6.6.0",
    "sass": "^1.32.8",
    "scss": "^0.2.4",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "codelyzer": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine-core": "~3.6.0",
